  "bugs": {
    "url": "https://github.com/B-0-B-B-Y/rock-tetris/issues"
  },
  "homepage": "https://github.com/B-0-B-B-Y/rock-tetris#readme",
  "dependencies": {},
  "devDependencies": {
    "electron": "^1.7.11",
    "electron-builder": "^19.52.1",
    "electron-packager": "^10.1.1",
    "electron-reload": "^1.2.2"
  }
}
